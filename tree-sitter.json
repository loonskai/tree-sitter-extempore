{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "extempore",
      "camelcase": "Extempore",
      "title": "Extempore",
      "scope": "source.extempore",
      "file-types": [
        "xtm"
      ],
      "injection-regex": "^extempore$",
      "class-name": "TreeSitterExtempore"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Extempore grammar for tree-sitter",
    "authors": [
      {
        "name": "Siarhei Lunski",
        "email": "loonskai@gmail.com",
        "url": "https://loonskai.com/"
      }
    ],
    "links": {
      "repository": "https://github.com/loonskai/tree-sitter-extempore"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}